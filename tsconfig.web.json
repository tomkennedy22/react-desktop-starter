{
	"extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
	"include": [
		"src/renderer/src/env.d.ts",
		"src/renderer/src/**/*",
		"src/backend/api.ts",
		"src/renderer/src/**/*.tsx",
		"src/preload/*.d.ts",
		"node_modules/@tanstack/"
	],
	"compilerOptions": {
		"composite": true,
		"jsx": "react-jsx",
		"paths": {
			"@renderer/*": [
				"./src/renderer/src/*"
			]
		}
	}
}